public class zhuanghuan{
    public static void main(String[] arge){
        int sum=0;
        for (int year=1900;year<=2015;year++){
            if(year%4==0&&year%100!=0||year%400==0){
                //sum=sum+366;
                sum+=366;
            }else{
                sum+=365;
            }
        }
        System.out.println("1900年到2015年相隔"+sum+"天");
    }
}
