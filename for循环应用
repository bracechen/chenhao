import java.util.Scanner;
public class zhuanghuan{
      public static void main(string[] args){
       Scanner in=new Scanner(System.in);
      int sum=0;
      for(int j=0;j<5;j++){
          System.out.println("请输入你的第"+(j+1)+"个成绩");
          int score=in,nextInt();
          sum=sum+score;
       }
          System.out.println("平均分为:"+sum/5);
      }
}
